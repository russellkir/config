[user]
    name = Russell Kirmayer
    email = russellkir@gmail.com
[core]
    editor = vim
    excludesfile =~/.gitignore
[web]
    browser = google-chrome
[alias]
    st = status
    cm = commit -m
    pom = push origin master
[color "branch"]
    current = yellow bold
    local = green bold
    remote = cyan bold
[color "diff"]
    meta = yellow bold
    frag = magenta bold
    old = red bold
    new = green bold
    whitespace = red reverse
[color "status"]
    added = green bold
    changed = yellow bold
    untracked = red bold
